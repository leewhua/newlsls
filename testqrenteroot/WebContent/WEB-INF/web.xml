<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>newqrenteroot</display-name>
  <filter>
    <filter-name>zfbfilter</filter-name>
    <filter-class>
	 	com.lsid.qrentry.filter.ZfbFilter
	</filter-class>
  </filter>
  <filter>
    <filter-name>wxfilter</filter-name>
    <filter-class>
		com.lsid.qrentry.filter.WxFilter
	</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>zfbfilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>wxfilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>
	 	 com.lsid.config.listener.AutoConfig
	 </listener-class>
  </listener>
</web-app>